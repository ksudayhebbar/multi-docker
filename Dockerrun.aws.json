{
    "AWSEBDocekrrunVersion":2,
    "containerDefinitions": 
    [ 
        {
        "name":"client",
        "image":"hebbaruday/multi-client",
        "hostname":"client",
        "essential":false
        },
        {
            "name":"server",
            "image":"hebbaruday/multi-server",
            "hostname":"api",
            "essential":false
        },
        {
            "name":"worker",
            "image":"hebbaruday/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
        "name": "nginx",
        "image": "hebbaruday/multi-nginx",
        "hostname": "worker",
        "essential": true,
        
        "portMappings" :[
            {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links":["client","server"]
      }
    ]

}